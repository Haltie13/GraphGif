var node_attrs=[shape="circle", color="red"];
graph ExampleGraph{
  node $node_attrs;
  A[label="Start"];
  B[label="Task"];
  C[label="End");
  A->B;
  B--C;
  C->A;
}
BFS(ExampleGraph, start_node:A);

var_ref       ::= "$" identifier
var node_style = [shape="circle", color="blue"];
A $node_style;
B $node_style;
