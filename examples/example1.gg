var node my_nodes = E, F, G;
var attributes atr1 = [weight = 4];
var attributes atr2 = [weight = 5];

directed graph G {
    $my_nodes;
    A, B, C;
    A -> B $atr2;
    B -> C $atr2;
    C -> A $atr1;
    B -> G $atr1;
    B -> F $atr1;
    E -> B [weight = 10];
    F-> E [weight = 1];
};

run G with (algorithm='dijkstra', start_node: 'A', output: 'custom_output/', animate: 'True');